{
  "version": 2,
  "functions": {
    "api/check-poa.js": {
      "maxDuration": 60
    },
    "api/analyze-poa.js": {
      "maxDuration": 60
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/public/$1"
    }
  ]
}
